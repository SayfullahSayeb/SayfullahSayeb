    <item>
      <title></title>
      <link>//localhost:1313/photography/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>//localhost:1313/photography/</guid>
      <description>&lt;!DOCTYPE html&gt;
&lt;html lang=&#34;en&#34;&gt;

&lt;head&gt;
  &lt;meta charset=&#34;UTF-8&#34;&gt;
  &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;
  &lt;title&gt;My Photography Gallery&lt;/title&gt;
  &lt;style&gt;
    .gallery-container {
      max-width: 1400px;
      margin: 0 auto;
    }

    .gallery {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 20px;
      margin: 20px auto;
    }

    .gallery-item {
      position: relative;
      background: #222332;
      border-radius: 5px;
      overflow: hidden;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .gallery-item img {
      width: 100%;
      height: 200px;
      object-fit: cover;
      display: block;
      cursor: pointer;
      margin: 0;
    }

    .image-details {
      font-size: .9em;
      color: var(--secondary);
      margin: 0 !important;
      padding: 10px;
    }

    .lightbox {
      display: none;
      position: fixed;
      z-index: 9999;
      top: 0;
      left: 0;
      align-content: center;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.9);
      backdrop-filter: blur(5px);
    }

    .lightbox-content {
      position: relative;
      max-width: 90%;
      max-height: 90vh;
      margin: 2% auto;
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    .lightbox-content img {
      max-width: 100%;
      max-height: 80vh;
      object-fit: contain;
      border-radius: 8px;
    }

    .lightbox-caption {
      color: white;
      margin-top: 20px;
      text-align: center;
      max-width: 600px;
    }

    .close {
      position: absolute;
      top: -30px;
      right: 20px;
      color: #fff;
      font-size: 40px;
      cursor: pointer;
      z-index: 1001;
    }

    @media (max-width: 768px) {
      .gallery {
        grid-template-columns: repeat(2, 1fr);
        gap: 15px;
      }

      .gallery-container {
        padding: 10px;
      }
    }

    @media (max-width: 480px) {
      .gallery {
        grid-template-columns: 1fr;
      }
    }
  &lt;/style&gt;
&lt;/head&gt;

&lt;body&gt;
  &lt;div class=&#34;gallery-container&#34;&gt;
    &lt;h2&gt;My Photography &lt;/h2&gt;
    &lt;p&gt;A collection of stunning photographs from various locations.&lt;/p&gt;</description>
    </item>
    <item>
      <title></title>
      <link>//localhost:1313/projects/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>//localhost:1313/projects/</guid>
      <description>&lt;!DOCTYPE html&gt;
&lt;html lang=&#34;en&#34;&gt;
&lt;head&gt;
    &lt;meta charset=&#34;UTF-8&#34;&gt;
    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;
    &lt;title&gt;Project Gallery&lt;/title&gt;
    &lt;link rel=&#34;stylesheet&#34; href=&#34;https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css&#34;&gt;
    &lt;style&gt;
        body {
            margin: 0;
            padding: 20px;
            background-color: #0f172a;
            color: #fff;
            font-family: system-ui, -apple-system, BlinkMacSystemFont, &#39;Segoe UI&#39;, Roboto, sans-serif;
        }

        .container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 20px;
        }

        .grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 25px;
        }

        .card {
            background: #1e293b;
            border-radius: 12px;
            overflow: hidden;
            transition: all 0.3s ease;
        }

        .card:hover {
            transform: translateY(-5px);
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
        }

        .card-image-container {
            position: relative;
            width: 100%;
            height: 200px;
            overflow: hidden;
        }

        .card-image {
            width: 100%;
            height: 100%;
            object-fit: cover;
            transition: transform 0.5s ease;
        }

        .card:hover .card-image {
            transform: scale(1.1);
        }

        .card-content {
            padding: 20px;
        }

        .card-title {
            font-size: 1.25em;
            font-weight: 600;
            color: #f1f5f9;
            margin-bottom: 12px;
        }

        .card-description {
            color: #94a3b8;
            font-size: 0.9em;
            line-height: 1.5;
            margin-bottom: 15px;
        }

        .tags {
            display: flex;
            flex-wrap: wrap;
            gap: 8px;
            margin-bottom: 15px;
        }

        .tag {
            background: #2d3a52;
            color: #60a5fa;
            padding: 5px 10px;
            border-radius: 15px;
            font-size: 0.75em;
            font-weight: 500;
        }

        .links {
            display: flex;
            justify-content: flex-start;
            gap: 15px;
            padding-top: 15px;
            border-top: 1px solid #334155;
        }

        .link-icon {
            color: #64748b;
            text-decoration: none;
            font-size: 1.2em;
            transition: all 0.3s ease;
        }

        .github-icon:hover {
            color: #f1f5f9;
        }

        .live-icon:hover {
            color: #4ade80;
        }

        @media (max-width: 1200px) {
            .grid {
                gap: 20px;
            }
            .card-content {
                padding: 15px;
            }
        }

        @media (max-width: 768px) {
            .grid {
                grid-template-columns: repeat(2, 1fr);
            }
        }

        @media (max-width: 480px) {
            .grid {
                grid-template-columns: 1fr;
            }
            .time-display {
                text-align: center;
            }
        }
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;div class=&#34;container&#34;&gt;
        &lt;div class=&#34;grid&#34;&gt;
            &lt;!-- Card 1 --&gt;
            &lt;div class=&#34;card&#34;&gt;
                &lt;div class=&#34;card-image-container&#34;&gt;
                    &lt;img class=&#34;card-image&#34; src=&#34;https://images.unsplash.com/photo-1506748686214-e9df14d4d9d0&#34; alt=&#34;Project 1&#34;&gt;
                &lt;/div&gt;
                &lt;div class=&#34;card-content&#34;&gt;
                    &lt;h3 class=&#34;card-title&#34;&gt;Project One&lt;/h3&gt;
                    &lt;p class=&#34;card-description&#34;&gt;
                        A modern web application showcasing the latest frontend technologies.
                    &lt;/p&gt;</description>
    </item>
  </channel>
