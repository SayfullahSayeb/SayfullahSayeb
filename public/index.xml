    <item>
      <title></title>
      <link>//localhost:1313/photography/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>//localhost:1313/photography/</guid>
      <description>&lt;!DOCTYPE html&gt;
&lt;html lang=&#34;en&#34;&gt;

&lt;head&gt;
  &lt;meta charset=&#34;UTF-8&#34;&gt;
  &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;
  &lt;title&gt;My Photography Gallery&lt;/title&gt;
  &lt;style&gt;
    .gallery-container {
      max-width: 1400px;
      margin: 0 auto;
    }

    .gallery {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 20px;
      margin: 20px auto;
    }

    .gallery-item {
      position: relative;
      background: #222332;
      border-radius: 5px;
      overflow: hidden;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .gallery-item img {
      width: 100%;
      height: 200px;
      object-fit: cover;
      display: block;
      cursor: pointer;
      margin: 0;
      filter: blur(10px);
      opacity: 0.3;
      transition: filter 0.5s ease-in-out, opacity 0.5s ease-in-out;
      background-size: cover;
      background-position: center;
    }

    .gallery-item img.loaded {
      filter: blur(0);
      opacity: 1;
    }

    .image-details {
      font-size: .9em;
      color: var(--secondary);
      margin: 0 !important;
      padding: 10px;
    }

    .lightbox {
      display: none;
      position: fixed;
      z-index: 9999;
      top: 0;
      left: 0;
      align-content: center;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.9);
      backdrop-filter: blur(5px);
    }

    .lightbox-content {
      position: relative;
      max-width: 90%;
      max-height: 90vh;
      margin: 2% auto;
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    .lightbox-content img {
      max-width: 100%;
      max-height: 80vh;
      object-fit: contain;
      border-radius: 8px;
    }

    .lightbox-caption {
      color: white;
      margin-top: 20px;
      text-align: center;
      max-width: 600px;
    }

    .close {
      position: absolute;
      color: #dfe6ff;
      font-size: 40px;
      cursor: pointer;
      border: 2px solid;
      border-radius: 100px;
      padding: 8px 10px;
      z-index: 1001;
    }

    @media (max-width: 768px) {
      .gallery {
        grid-template-columns: repeat(2, 1fr);
        gap: 15px;
      }

      .gallery-container {
        padding: 10px;
      }
    }

    @media (max-width: 480px) {
      .gallery {
        grid-template-columns: 1fr;
      }
    }
  &lt;/style&gt;
&lt;/head&gt;

&lt;body&gt;
  &lt;div class=&#34;gallery-container&#34;&gt;
    &lt;h2&gt;My Photography &lt;/h2&gt;
    &lt;p&gt;A collection of stunning photographs from various locations.&lt;/p&gt;</description>
    </item>
    <item>
      <title></title>
      <link>//localhost:1313/projects/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>//localhost:1313/projects/</guid>
      <description>&lt;!DOCTYPE html&gt;
&lt;html lang=&#34;en&#34;&gt;

&lt;head&gt;
  &lt;meta charset=&#34;UTF-8&#34;&gt;
  &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;
  &lt;title&gt;Project Gallery&lt;/title&gt;
  &lt;link rel=&#34;stylesheet&#34; href=&#34;https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css&#34;&gt;
  &lt;style&gt;
    .post-content img,
    .post-content a,
    .post-content h3 {
      margin: 0% !important;
      box-shadow: none !important;
    }

    .grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 20px;
      width: 100%;
    }

    .card {
      background: #1a1b27;
      border-radius: 12px;
      overflow: hidden;
    }

    .card-image-container {
      position: relative;
      width: 100%;
      padding-top: 56.25%;
      overflow: hidden;
    }

    .card-image {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .card-content {
      padding: 16px;
    }

    .card-title {
      font-size: 1.25em;
      font-weight: 600;
      color: #f1f5f9;
      margin-bottom: 12px;
    }

    .card-description {
      color: #94a3b8;
      font-size: 0.9em;
      line-height: 1.5;
      margin-bottom: 15px;
    }

    .tags {
      display: flex;
      flex-wrap: wrap;
      gap: 8px;
      margin-bottom: 15px;
    }

    .tag {
      background: #2d3a52;
      color: #60a5fa;
      padding: 1px 10px;
      border-radius: 15px;
      font-size: 0.75em;
      font-weight: 500;
    }

    .links {
      display: flex;
      justify-content: flex-start;
      gap: 15px;
      padding-top: 15px;
      border-top: 1px solid #334155;
    }

    .link-icon {
      color: #64748b;
      text-decoration: none;
      font-size: 1.2em;
    }

    .github-icon:hover,
    .live-icon:hover {
      color: #2d2d3b;
    }

    /* Responsive breakpoints */
    @media (max-width: 1024px) {
      .grid {
        grid-template-columns: repeat(2, 1fr);
      }
    }

    @media (max-width: 640px) {
      .grid {
        grid-template-columns: 1fr;
      }
    }
  &lt;/style&gt;
&lt;/head&gt;

&lt;body&gt;
  &lt;div class=&#34;container&#34;&gt;
    &lt;div class=&#34;grid&#34;&gt;
      &lt;div class=&#34;card&#34;&gt;
        &lt;div class=&#34;card-image-container&#34;&gt;
          &lt;img class=&#34;card-image&#34; src=&#34;https://images.unsplash.com/photo-1506765515384-028b60a970df&#34; alt=&#34;Project&#34;&gt;
        &lt;/div&gt;
        &lt;div class=&#34;card-content&#34;&gt;
          &lt;h3 class=&#34;card-title&#34;&gt;Project Title&lt;/h3&gt;
          &lt;p class=&#34;card-description&#34;&gt;Project description here.&lt;/p&gt;</description>
    </item>
  </channel>
